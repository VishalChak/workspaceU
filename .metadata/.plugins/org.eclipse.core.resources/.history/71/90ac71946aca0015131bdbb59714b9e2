import java.util.Scanner;

public class CounterGame {
	static Scanner scan = new Scanner (System.in);
	public static void main(String args[]){
		int t= scan.nextInt(),n;
		boolean b;
		for(int i=0;i<t;i++){
			n= scan.nextInt();
			if((n&-n)==n){
				n=n/2;
			}
			else 
				n= (int) (n-Math.pow((int)Math.sqrt(n), 2));
			}
		}
	}


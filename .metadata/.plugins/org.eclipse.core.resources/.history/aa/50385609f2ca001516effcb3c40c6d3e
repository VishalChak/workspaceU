import java.util.Scanner;

public class CalatanNo {
	static Scanner scan = new Scanner(System.in);
	public static void main(String args[]){
		
		int n=scan.nextInt();
		System.out.print(directEquation(n)+" ");
		
	}
	
	public static int directEquation(int n){
		int ans=1;
		for(int i=2*n;i>n;i--){
			ans*=i;
		}
		for(int i=n;i>=1;i--){
			
		}
		return ans/(n+1);
	}
}

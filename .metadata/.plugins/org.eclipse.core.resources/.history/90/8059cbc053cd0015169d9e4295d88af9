import java.util.Scanner;

public class PairsFromTwoArr {
	static int a[],b[];
	static Scanner scan = new Scanner (System.in);
	public static void main(String args[]){
		int n=scan.nextInt();
		for(int i=0;i<n;i++)
			a[i]= scan.nextInt();
		n=scan.nextInt();
		for(int i=0;i<n;i++)
			b[i]= scan.nextInt();
	}
}

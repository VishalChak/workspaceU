import java.util.Scanner;

public class maxContiSubsequence {
 static Scanner scan = new Scanner(System.in);
 static int arr[];
 public static void main(String args[]){
	 int n=scan.nextInt(),maxSum=0,curSum=0;
	 arr= new int [n];
	 for(int i=0;i<n;i++)
		 arr[i]= scan.nextInt();
	 for(int i=0;i<n;i++){
		 curSum+=arr[i];
		 if(curSum<0)
			 curSum=0;
		 if(maxSum<curSum)
			 maxSum=curSum;
	 }
	 System.out.println(maxSum);
 }
}
